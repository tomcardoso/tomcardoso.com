@font-face {
  font-family: 'din_bold';
  src: image-url('din-bold-webfont.eot');
  src: image-url('din-bold-webfont.eot?#iefix') format('embedded-opentype'),
       image-url('din-bold-webfont.woff') format('woff'),
       image-url('din-bold-webfont.ttf') format('truetype'),
       image-url('din-bold-webfont.svg#dinbold') format('svg');
  font-weight: normal;
  font-style: normal;
}

